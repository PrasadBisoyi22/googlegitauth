# Frontend Environment Variables for Google/GitHub OAuth Login

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000
REACT_APP_API_PREFIX=/api

# Authentication Endpoints
REACT_APP_GOOGLE_AUTH_URL=http://localhost:5000/api/auth/google
REACT_APP_GITHUB_AUTH_URL=http://localhost:5000/api/auth/github
REACT_APP_LOGIN_CALLBACK_URL=http://localhost:5000/api/auth/google/callback
REACT_APP_SIGNUP_CALLBACK_URL=http://localhost:5000/api/google-auth/login
REACT_APP_USER_PROFILE_URL=http://localhost:5000/api/auth/me

# Frontend Configuration
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_LOGIN_PAGE_URL=http://localhost:3000/login
REACT_APP_SIGNUP_PAGE_URL=http://localhost:3000/signup
REACT_APP_DASHBOARD_URL=http://localhost:3000/dashboard

# OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id_here

# Development Settings
REACT_APP_DEV_MODE=true
REACT_APP_DEBUG=true

# Build Configuration
REACT_APP_BUILD_PATH=build
REACT_APP_PUBLIC_URL=/

# Optional: Analytics and Monitoring
# REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
# REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Optional: Feature Flags
# REACT_APP_ENABLE_GOOGLE_AUTH=true
# REACT_APP_ENABLE_GITHUB_AUTH=true
# REACT_APP_ENABLE_EMAIL_AUTH=true
